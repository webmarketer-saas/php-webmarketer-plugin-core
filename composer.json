{
  "name": "webmarketer/webmarketer-plugin-core-php",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "webmarketer/webmarketer-php": "^3.4",
    "nyholm/psr7": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.27 || >= 6.5.14"
  },
  "autoload": {
    "psr-4": {
      "WebmarketerPluginCore\\": "src/",
      "WebmarketerPluginCore\\Extractors\\": "src/extractors",
      "WebmarketerPluginCore\\Mappable\\": "src/mappable",
      "WebmarketerPluginCore\\Mapping\\": "src/mapping",
      "WebmarketerPluginCore\\Queue\\": "src/queue",
      "WebmarketerPluginCore\\Services\\": "src/services",
      "WebmarketerPluginCore\\Settings\\": "src/settings"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
